{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "logout": true,
          "create": true
        }
      }
    }
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "Metric": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "getFiles": true,
          "spreadsheets": true
        }
      }
    }
  },
  "Segment": {
    "dataSource": "mongodb",
    "public": false
  },
  "Unique": {
    "dataSource": "mongodb",
    "public": false
  },
  "Customer": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true
        }
      }
    }
  },
  "Storage": {
    "dataSource": "storage",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "upload": true
        }
      }
    }
  }
}
